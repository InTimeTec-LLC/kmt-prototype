<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "suppressions_1_1.dtd">

<suppressions>
	<!-- Suppress JavadocPackage in the test packages -->
	<suppress checks="JavadocPackage" files=".*[\\/]src[\\/](test)[\\/]" />
	<suppress checks="JavadocType" files=".*[\\/]src[\\/](test)[\\/]" />
	<suppress checks="JavadocVariable" files=".*[\\/]src[\\/](test)[\\/]" />
	<suppress checks="JavadocStyle" files=".*[\\/]src[\\/](test)[\\/]" />
	<suppress checks="Javadoc" files=".*[\\/]src[\\/](test)[\\/]" />
	<suppress checks="JavadocMethod" files=".*[\\/]src[\\/](test)[\\/]" />

	<!-- Suppress all checkstyle for autogenerated jaxws.managed package -->
	<suppress checks="[a-zA-Z0-9]*"
		files="([^(delegate)])+([a-z]*[\\/]jaxws[\\/]managed[\\/])+" />
</suppressions>